{
  "name": "shop-feuerschutz-frontend",
  "version": "1.0.0",
  "description": "A react version of the shop.feuerschutz.ch frontend",
  "private": true,
  "main": "src/index.js",
  "author": "Nico Hauser",
  "license": "UNLICENSED",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "PORT=8080 node server.js",
    "build": "next build",
    "build-server": "webpack --config webpack-build-server.config.js",
    "dev-server": "npm run build && npm run build-server && node dist/server/index.js",
    "extract-messages": "yarn extract-messages-components && yarn extract-messages-pages && yarn extract-messages-utilities && yarn extract-messages-i18n",
    "extract-messages-components": "formatjs extract ./components/*.tsx ./components/**/*.tsx --out-file ./messages/components.json",
    "extract-messages-pages": "formatjs extract ./pages/*.tsx ./pages/**/*.tsx --out-file ./messages/pages.json",
    "extract-messages-utilities": "formatjs extract ./utilities/*.tsx ./utilities/**/*.tsx --out-file ./messages/utilities.json",
    "extract-messages-i18n": "formatjs extract ./i18n/*.ts ./i18n/**/*.ts --out-file ./messages/i18n.json",
    "collect-messages": "node scripts/collect-i18n-strings.js",
    "generate-messages": "rm -r messages/* && yarn extract-messages && yarn collect-messages && node scripts/update-18n.js",
    "generate-types": "gq $(node scripts/get-env-variable.js API_URL) --introspect > schema.graphql && node scripts/convert-schema.js && graphql-codegen --config codegen.config.yml",
    "analysis": "webpack-bundle-analyzer ./dist/stats.json",
    "prettier": "prettier --write .",
    "test": "echo 'not implemented'",
    "deploy": "node scripts/generate-i18n-pages.js && yarn build",
    "analyze": "ANALYZE=true next build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@formatjs/cli": "^2.13.1",
    "@formatjs/intl-pluralrules": "^1.3.2",
    "@formatjs/intl-relativetimeformat": "^5.1.0",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@next/bundle-analyzer": "^9.5.5",
    "@types/color": "^3.0.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^16.0.5",
    "@types/react-images": "^0.5.3",
    "@types/react-select": "^3.0.19",
    "@types/yup": "^0.29.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-emotion": "^10.0.33",
    "balloon-css": "^0.5.0",
    "compression": "^1.7.3",
    "create-emotion-server": "^10.0.27",
    "decode-html": "^2.0.0",
    "emotion": "^10.0.27",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.17.1",
    "formik": "^0.11.11",
    "full-icu": "^1.3.0",
    "ghooks": "^2.0.4",
    "glob": "^7.1.6",
    "graphql": "^15.0.0",
    "graphqurl": "^0.3.3",
    "html-entities": "^1.2.1",
    "iframe-resizer": "^3.6.1",
    "intersection-observer": "^0.5.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.8.9",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "next": "^9.5.3",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^4.1.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^7.0.0",
    "object-sizeof": "^1.2.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-autosuggest": "^9.3.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-icons": "^3.6.1",
    "react-iframe-resizer-super": "^0.2.0",
    "react-images": "^0.5.19",
    "react-intl": "^3.6.0",
    "react-lazy-images": "^1.1.0",
    "react-motion": "^0.5.2",
    "react-paginate": "^5.0.0",
    "react-progress-button": "^5.1.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.8.3",
    "react-transition-group": "1.x",
    "rimraf": "^2.6.2",
    "schema-dts": "^0.6.0",
    "style-loader": "^0.19.0",
    "swr": "^0.2.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "yup": "^0.26.6"
  }
}
