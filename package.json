{
  "name": "shop-feuerschutz-frontend",
  "version": "1.0.0",
  "description": "A react version of the shop.feuerschutz.ch frontend",
  "private": true,
  "main": "src/index.js",
  "author": "Nico Hauser",
  "license": "UNLICENSED",
  "scripts": {
    "start": "HOSTNAME=127.0.0.1 PORT=8080 NODE_ENV=production node server.js",
    "dev": "HOSTNAME=127.0.0.1 PORT=8080 node server.js",
    "build": "next build",
    "lint": "next lint",
    "generate-sitemap": "next-sitemap --config ./next-sitemap.js",
    "build-server": "webpack --config webpack-build-server.config.js",
    "dev-server": "npm run build && npm run build-server && node dist/server/index.js",
    "extract-messages": "yarn extract-messages-components && yarn extract-messages-pages && yarn extract-messages-utilities && yarn extract-messages-i18n",
    "extract-messages-components": "formatjs extract './components/*.tsx' './components/**/*.tsx' --out-file ./messages/components.json",
    "extract-messages-pages": "formatjs extract './pages/*.tsx' './pages/**/*.tsx' --out-file ./messages/pages.json",
    "extract-messages-utilities": "formatjs extract './utilities/*.tsx' './utilities/**/*.tsx' --out-file ./messages/utilities.json",
    "extract-messages-i18n": "formatjs extract './i18n/*.ts' './i18n/**/*.ts' --out-file ./messages/i18n.json",
    "collect-messages": "node scripts/collect-i18n-strings.js",
    "generate-messages": "rm -r messages/* && yarn extract-messages && yarn collect-messages && node scripts/update-18n.js",
    "generate-types": "gq $(node scripts/get-env-variable.js API_URL) --introspect -H 'Content-Type: application/json' > schema.graphql && node scripts/convert-schema.js && graphql-codegen --config codegen.config.yml",
    "analysis": "webpack-bundle-analyzer ./dist/stats.json",
    "prettier": "prettier --write .",
    "test": "echo 'not implemented'",
    "deploy": "node scripts/generate-i18n-pages.js && yarn build",
    "analyze": "ANALYZE=true next build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "resolutions": {
    "graphql": "15.5.1"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@formatjs/cli": "^2.13.1",
    "@formatjs/intl-pluralrules": "^1.3.2",
    "@formatjs/intl-relativetimeformat": "^5.1.0",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@next/bundle-analyzer": "^13.0.5",
    "@next/font": "^13.0.5",
    "@types/color": "^3.0.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^14.0.5",
    "@types/react": "^17.0.20",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-select": "^3.0.22",
    "@types/yup": "^0.29.0",
    "autoprefixer": "^9.7.1",
    "balloon-css": "^0.5.0",
    "decode-html": "^2.0.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "express": "^4.17.1",
    "formik": "^2.2.6",
    "ghooks": "^2.0.4",
    "glob": "^7.1.6",
    "graphql": "15.5.1",
    "graphqurl": "^1.0.1",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "next": "^13.0.5",
    "next-sitemap": "^3.1.32",
    "next-transpile-modules": "^10.0.0",
    "normalize.css": "^7.0.0",
    "object-sizeof": "^1.2.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "react-autosuggest": "^9.3.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^18.2.0",
    "react-icons": "^3.6.1",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "^3.6.0",
    "react-progress-button": "^5.1.0",
    "react-quill": "^2.0.0-beta.4",
    "react-select": "^3.1.0",
    "react-spinners": "^0.8.3",
    "rimraf": "^2.6.2",
    "sass": "^1.45.2",
    "schema-dts": "^0.6.0",
    "style-loader": "^0.19.0",
    "swr": "^1.2.1",
    "typescript": "^4.4.2",
    "xlsx": "^0.18.5",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0"
  }
}
