    "dev": "webpack-dev-server",
    "build": "yarn extract-messages && yarn collect-messages && webpack --config webpack-build.config.js",
    "build-server": "webpack --config webpack-build-server.config.js",
    "dev-server": "npm run build && npm run build-server && node dist/server/index.js",
    "extract-messages": "NODE_ENV=production babel ./src  --out-file /dev/null",
    "collect-messages": "NODE_ENV=production node scripts/collect-i18n-strings.js",
    "analysis": "webpack-bundle-analyzer ./dist/stats.json",
    "docs": "rm -rf ./docs/* && jsdoc src -c jsdoc.json",
    "prettier": "prettier --print-width 80 --tab-width 2 --trailing-comma none --arrow-parens avoid --write src/**/*",
    "lint": "eslint --ext .jsx --ext .js src/",
    "test": "echo 'not implemented'"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prettier && npm run lint --fix",
      "pre-push": "npm run test"
    }
  },
