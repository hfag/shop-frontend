{
  "name": "shop-feuerschutz-frontend",
  "version": "1.0.0",
  "description": "A react version of the shop.feuerschutz.ch frontend",
  "private": true,
  "main": "src/index.js",
  "author": "Nico Hauser",
  "license": "UNLICENSED",
  "scripts": {
    "start": "HOSTNAME=127.0.0.1 PORT=8080 NODE_ENV=production node server.js",
    "dev": "HOSTNAME=127.0.0.1 PORT=8080 node server.js",
    "build": "next build",
    "lint": "next lint",
    "generate-sitemap": "next-sitemap --config ./next-sitemap.js",
    "build-server": "webpack --config webpack-build-server.config.js",
    "dev-server": "npm run build && npm run build-server && node dist/server/index.js",
    "extract-messages": "yarn extract-messages-components && yarn extract-messages-pages && yarn extract-messages-utilities && yarn extract-messages-i18n && yarn extract-messages-validation",
    "extract-messages-components": "formatjs extract './components/*.tsx' './components/**/*.tsx' --out-file ./messages/components.json",
    "extract-messages-pages": "formatjs extract './pages/*.tsx' './pages/**/*.tsx' --out-file ./messages/pages.json",
    "extract-messages-utilities": "formatjs extract './utilities/*.tsx' './utilities/**/*.tsx' --out-file ./messages/utilities.json",
    "extract-messages-i18n": "formatjs extract './i18n/*.ts' './i18n/**/*.ts' --out-file ./messages/i18n.json",
    "extract-messages-validation": "formatjs extract './validation/*.ts' './validation/**/*.ts' --out-file ./messages/validation.json",
    "collect-messages": "node scripts/collect-i18n-strings.js",
    "generate-messages": "rm -r messages/* && yarn extract-messages && yarn collect-messages && node scripts/update-18n.js",
    "generate-types": "gq $(node scripts/get-env-variable.js API_URL) --introspect -H 'Content-Type: application/json' > schema.graphql && node scripts/convert-schema.js && graphql-codegen --config codegen.config.yml",
    "analysis": "webpack-bundle-analyzer ./dist/stats.json",
    "prettier": "prettier --write .",
    "test": "echo 'not implemented'",
    "deploy": "node scripts/generate-i18n-pages.js && yarn build",
    "analyze": "ANALYZE=true next build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "resolutions": {
    "@types/react": "18"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@formatjs/cli": "^6.7.4",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@next/bundle-analyzer": "^13.0.5",
    "@types/color": "^4.2.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.2",
    "@types/react": "18",
    "@types/react-autosuggest": "^10.1.11",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "18",
    "autoprefixer": "^10.4.22",
    "balloon-css": "^1.2.0",
    "decode-html": "^2.0.0",
    "eslint-config-next": "^14.2.33",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "express": "^4.17.1",
    "formik": "^2.4.9",
    "ghooks": "^2.0.4",
    "glob": "^13.0.0",
    "graphql": "^16.12.0",
    "graphqurl": "^2.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "next": "^14.2.33",
    "next-sitemap": "^3.1.32",
    "normalize.css": "^8.0.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^2.3.2",
    "react": "18",
    "react-autosuggest": "^10.1.0",
    "react-dom": "18",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "^7.1.14",
    "react-progress-button": "^5.1.0",
    "react-quill": "^2.0.0",
    "react-select": "^5.10.2",
    "react-spinners": "^0.17.0",
    "rimraf": "^6.1.2",
    "sass": "^1.95.1",
    "schema-dts": "^1.1.5",
    "swr": "^2.3.7",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
